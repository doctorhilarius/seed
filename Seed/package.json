{
  "name": "www.seed.com",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "e2e": "ng e2e",
    "lint": "tslint \"src/**/*.ts\"",
    "local-ssl": "npm run local-ngsw-worker && ng s --port 44301 --ssl true",
    "local-ngsw-worker": "copy .\\node_modules\\@angular\\service-worker\\ngsw-worker.js .\\src\\ngsw-worker.js /Y && npm run local-ngsw-config",
    "local-ngsw-config": "node_modules/.bin/ngsw-config src ./src/ngsw-config.json",
    "dev": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build --aot -t=production -e=dev",
    "prod": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build --aot --build-optimizer -t=production -e=prod -vc=true -cc=false && npm run prod-ngsw-worker",
    "prod-ngsw-worker": "copy .\\node_modules\\@angular\\service-worker\\ngsw-worker.js .\\wwwroot\\ngsw-worker.js /Y && npm run prod-ngsw-config",
    "prod-ngsw-config": "node_modules/.bin/ngsw-config wwwroot ./src/ngsw-config.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~5.2.4",
    "@angular/cdk": "~5.2.1",
    "@angular/common": "~5.2.4",
    "@angular/compiler": "~5.2.4",
    "@angular/core": "~5.2.4",
    "@angular/forms": "~5.2.4",
    "@angular/http": "~5.2.4",
    "@angular/material": "~5.2.1",
    "@angular/platform-browser": "~5.2.4",
    "@angular/platform-browser-dynamic": "~5.2.4",
    "@angular/platform-server": "~5.2.4",
    "@angular/router": "~5.2.4",
    "@angular/service-worker": "~5.2.4",
    "core-js": "~2.5.3",
    "hammerjs": "~2.0.8",
    "rxjs": "~5.5.6",
    "zone.js": "~0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "~1.6.8",
    "@angular/compiler-cli": "~5.2.4",
    "@angular/language-service": "~5.2.4",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~9.4.5",
    "codelyzer": "~4.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "~1.4.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "~0.2.2",
    "node-sass": "~4.7.2",
    "protractor": "~5.3.0",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.7.1"
  }
}
